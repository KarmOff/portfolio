block title

//-=== mixins ====
include mixins/mixins.pug
doctype html
html
  head
    meta(charset="utf-8")
    title #{title}
    meta(content="" name="author")
    meta(content="" name="description")
    meta(content="" name="keywords")
    meta(content="width=device-width, initial-scale=1.0" name="viewport")
    meta(content="ie=edge" http-equiv="x-ua-compatible")

    //- favicon block start
    //- favicon block end
    
    

    link(rel='stylesheet' href='assets/css/foundation.css')
    link(rel='stylesheet' href='assets/css/app.css')
    script(src='assets/js/foundation.js' defer)
    script(src='assets/js/app.js' defer)

    
    script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyChz-R3zF1RQTct0VvR8qvg0GDRH_YBVIA&extension=.js")
    script(src="https://cdn.mapkit.io/v1/infobox.js")
    link(href="https://fonts.googleapis.com/css?family=Roboto:300,400" rel="stylesheet") 
    link(href="https://cdn.mapkit.io/v1/infobox.css" rel="stylesheet")
    <!--[if lt IE 9]>
    script(src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js")
    <![endif]-->

body
    .global
        .wrapper
            block content
        
        block footer